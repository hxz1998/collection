<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane minHeight="800.0" minWidth="1000.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.summer.summerdesktop.ThreadDumpViewController">
    <center>
        <AnchorPane prefHeight="574.0" prefWidth="748.0">
            <children>
                <TextArea fx:id="threadDumpInfo" editable="false" prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <font>
                  <Font name="Courier New Bold" size="13.0" />
               </font></TextArea>
            </children>
        </AnchorPane>
    </center>
    <top>
        <ToolBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <items>
                <Button fx:id="printPDF" mnemonicParsing="false" onAction="#onClickPrintPDF" text="打印PDF" />
            </items>
        </ToolBar>
    </top>
</BorderPane>
